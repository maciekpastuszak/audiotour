	<script src="jquery.js"></script>
	<script src="scripts.js"></script>
	<script type="text/javascript">
		$(function(){

			$('.play-pause').click(function(){
				var playerID = $(this).attr('id');
				if($(this).hasClass('playing')){
					$('audio#'+playerID).trigger('pause');
					$(this).removeClass('playing');
					$('.play-pause#'+playerID).css('background-image', 'url("img/play.png")');
				}
				else{
					$('audio').each(function(){
						$(this).trigger('pause');
						$('.play-pause#'+$(this).attr('id')).css('background-image', 'url("img/play.png")').removeClass('playing');
					});

					$('audio#'+playerID).trigger('play');
					$('.play-pause#'+playerID).css('background-image', 'url("img/pause.png")').addClass("playing");
				}
			});
			$('audio').each(function(){

				$(this).trigger('load');
				var player = document.querySelector('audio#' + $(this).attr('id'));

				player.addEventListener("timeupdate", function() {
					var currentTime = player.currentTime;
					var duration = player.duration;
					$('.seekbar#'+player.id).stop().animate({width: currentTime/duration*100+'%'}, 500);
				});
			});
		});

	</script>

</body>
</html>
